        game:GetService("ReplicatedStorage").Assets.RemoteEvents.BuyUpgrade:FireServer(unpack(args))
        task.wait(10)

        local args = {[1] = "3_4",[2] = true}
        game:GetService("ReplicatedStorage").Assets.RemoteEvents.BuyUpgrade:FireServer(unpack(args))
        task.wait(10)

        local args = {[1] = "3_1",[2] = true}
        game:GetService("ReplicatedStorage").Assets.RemoteEvents.BuyUpgrade:FireServer(unpack(args))
        task.wait(10)

        local args = {[1] = "3_2",[2] = true}
        game:GetService("ReplicatedStorage").Assets.RemoteEvents.BuyUpgrade:FireServer(unpack(args))
