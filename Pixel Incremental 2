local ui = Library:NewWindow("Pixel Incremental 2")
local a = ui:NewSection("Farm")
local b = ui:NewSection("Buy Starter")
local c = ui:NewSection("Buy Advanced")
local d = ui:NewSection("Buy idk (later)")

a:CreateToggle("Auto Collect", function(value)
    _G.Auto_Collect = value
    if value then
        Collect()
    end
end)

a:CreateToggle("Auto Click", function(value)
    _G.Auto_Click = value
    if value then
        Click()
    end
end)

b:CreateToggle("Auto Buy RNG Upgrades", function(value)
    _G.Auto_Buy_RNG = value
    if value then
        Buy_RNG()
    end
end)

b:CreateToggle("Auto Buy Points Upgrades", function(value)
    _G.Auto_Buy_Point = value
    if value then
        Buy_Point()
    end
end)

b:CreateToggle("Auto Buy Pixels Upgrades", function(value)
    _G.Auto_Buy_Pixel = value
    if value then
        Buy_pixel()
    end
end)

b:CreateToggle("Auto Buy Prestige Upgrades", function(value)
    _G.Auto_Buy_Prestige = value
    if value then
        Buy_Prestige()
    end
end)

b:CreateToggle("Auto Buy Click Upgrades", function(value)
    _G.Auto_Buy_Click = value
    if value then
        Buy_Click()
    end
end)

b:CreateToggle("Auto Buy Gems Upgrades", function(value)
    _G.Auto_Buy_Gems = value
    if value then
        Buy_Gems()
    end
end)

c:CreateToggle("Auto Buy Research Upgrades", function(value)
    _G.Auto_Buy_Research = value
    if value then
        Buy_Research()
    end
end)
